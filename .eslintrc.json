{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "quotes": [2,"double",{"avoidEscape": true, "allowTemplateLiterals": true}],
      "no-unused-vars": "off"
    }
}
